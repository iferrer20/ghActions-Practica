{
  "extends": ["next/core-web-vitals", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "quotes": ["error", "double"],
    "curly":"error",
    "default-case-last": "error",
    "no-var": "error"
  },
  "settings": {
    "next": {
      "rootDir": ["./**"]
    }
  }
}
